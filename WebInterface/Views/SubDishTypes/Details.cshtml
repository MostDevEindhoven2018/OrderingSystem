@model WebInterface.Models.SubDishType

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<h2>@Model.SubType Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Subcategory:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SubType)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-default" asp-route-id="@Model.SubDishTypeID">Edit</a> |
    <a asp-action="Index">Go Back to Subcategories Overview</a>
</div>
