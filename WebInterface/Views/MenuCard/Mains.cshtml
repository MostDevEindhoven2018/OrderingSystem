@model List<DishType>


@{
    ViewData["Title"] = "Mains";
    Layout = "~/Views/Shared/_MenuCardLayout.cshtml";
}

<h2>Main Course</h2>

<ol>

    @foreach (DishType d in Model)
    {
        <li>  @d.Name </li>


    }

</ol>

    <input type="button" value="Go Back" onclick="location.href='@Url.Action("Starters", "MenuCard" )'" />

    <input type="button" value="Proceed To Dessert" onclick="location.href='@Url.Action("Desserts", "MenuCard" )'" />

    <input type="button" value="Place Order" onclick="location.href='@Url.Action("OrderOverview", "MenuCard" )'" />

    


